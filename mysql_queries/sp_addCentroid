USE uym2$theimgame2016;
DROP procedure IF EXISTS `sp_addCentroid`;

DELIMITER $$
CREATE PROCEDURE `sp_addCentroid`(
	IN p_topleftX int,
	   p_topleftY int,
	   p_bottomrightX int,
	   p_bottomrightY int
)
BEGIN
	INSERT INTO tbl_centroid(
	topleft_x,topleft_y,bottomright_x,bottomright_y)
	VALUES(p_topleftX,p_topleftY,p_bottomrightX,p_bottomrightY);
END$$

DELIMITER ;
