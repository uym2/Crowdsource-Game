DROP PROCEDURE IF EXISTS `sp_getCentroidsByImgID`;

DELIMITER $$

CREATE PROCEDURE `sp_getCentroidsByImgID`(
	IN p_imgID int
)
BEGIN
	SELECT DISTINCT centroid_id, AVG(topleft_x),AVG(topleft_y),AVG(bottomright_x),AVG(bottomright_y)
	FROM tbl_objmark
	WHERE img_id=p_imgID
	GROUP BY centroid_id;
	END$$

DELIMITER ;
